{
    "name": "tvdijen/simplesamlphp-module-sqlauth",
    "description": "This is a basic authentication module for authenticating a user against a SQL database",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "sqlauth"],
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        }
    },
    "extra": {
        "ssp-mixedcase-module-name": "SqlAuth"
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\SqlAuth\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": ">=7.2",
        "simplesamlphp/composer-module-installer": "~1.1",
        "simplesamlphp/simplesamlphp": "dev-master",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^0.1.3"
    },
    "support": {
        "issues": "https://github.com/tvdijen/simplesamlphp-module-sqlauth/issues",
        "source": "https://github.com/tvdijen/simplesamlphp-module-sqlauth"
    }
}
